<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.xiaqi.entity">
	<class name="ContactMan" table="contactMan" >
		<id name="contactMan_id"  >
			<generator class="native"></generator>
		</id>
		<property name="customer_id"></property>
		<property name="contactManName"></property>
		<property name="sex"></property>
		<property name="phone"></property>
		<property name="telephone"></property>
		
	</class>
</hibernate-mapping>